draw_set_color(c_white); //Sets the menu buttons with a white color

draw_text(200, 100, "Tank Arena");

draw_set_font(font_menu); //Sets up the selected font
var i;
for(i = 0; i&lt; array_length_1d(menu); i+=1){ //Creates a loop for the array to draw out the menu
