    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>var menu_move = 0;

menu_move -= max(keyboard_check_pressed(vk_up), keyboard_check_pressed(ord("W")), 0);
//"max" selects the highest value, given the variables. The keyboard checks
//will return a value of 1 if true, and will return a value of 0 if false.
//Meaning the "move" has a negative counter that will either count down by 1,
//or count up by 0.
menu_move += max(keyboard_check_pressed(vk_down), keyboard_check_pressed(ord("S")), 0);
//Same thing, except with a positive counter

if(menu_move != 0){
mpos += menu_move;
if(mpos &lt; 0) mpos = array_length_1d(menu) -1; 
if(mpos &gt; array_length_1d(menu)-1) mpos = 0;
}
//These make it so that if you go above or below the limited array, it will just loop
//back around, so the cursor is always selecting a valid item.

var push_select; //This will track if the select key(Space or Enter) has been pushed or not.
push_select = max(keyboard_check_released(vk_enter),keyboard_check_released(vk_space), 0);

if(push_select == 1) menu_script();
//This will run the menu script if a selection has been made.

</string>
          </argument>
        </arguments>
      </action>
    </event>
