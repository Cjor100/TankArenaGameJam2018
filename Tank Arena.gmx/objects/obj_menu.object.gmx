            <kind>1</kind>
            <string>menu[0] = "Start"; //This is an array to store all the variables for the menu
menu[1] = "Tutorial";
menu[2] = "Credits";
menu[3] = "Quit";

space = 64; //This variable is for the set amount of space between each menu item
mpos = 0; //This is the menu cursor position

audio_stop_sound(sound_game_music)
audio_stop_sound(sound_move1)
audio_stop_sound(sound_move2)
audio_stop_sound(sound_idle1)
audio_stop_sound(sound_idle2)

global.flawless = 0;

