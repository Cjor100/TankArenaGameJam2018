            <kind>1</kind>
            <string>//Forward player control
if(keyboard_check_pressed(ord('W'))){ forward = true; }
if(keyboard_check_released(ord('W'))){forward = false;}

//Down player control
if(keyboard_check_pressed(ord('S'))){ down = true; }
if(keyboard_check_released(ord('S'))){ down = false;}

//Left player control
if(keyboard_check_pressed(ord('A'))){ left = true; }
if(keyboard_check_released(ord('A'))){ left = false;}

//Right player control
if(keyboard_check_pressed(ord('D'))){ right = true; }
if(keyboard_check_released(ord('D'))){ right = false;}

//Attempt assingment
attempt_right=right
attempt_left=left
attempt_forward=forward
attempt_down=down

//Move
if(y &gt; 32){
if(attempt_forward){y-= player_speed;player_direction = 90;}
}
if(y &lt; 736){
if(attempt_down){y+=player_speed;player_direction = 270;}
}
if(x &gt; 32){
if(attempt_left){ x-=player_speed;player_direction = 180;}
}
if(x &lt; 992){
if(attempt_right){ x+=player_speed;player_direction = 0;}
}

if(right &amp;&amp; forward){no_shoot = true; player_direction=45;} //Up-Right angle control
if(forward &amp;&amp; left){ no_shoot = true; player_direction=135;} //Up-Left angle control
if(left &amp;&amp; down){ no_shoot = true; player_direction=225;} //Down-Left angle control
if(down &amp;&amp; right){ no_shoot = true; player_direction=315;} //Down-Right angle control

if(timer &lt;= 0){ //The timer prevents the player from being able to spam shoot
start_time = false;
//if(no_shoot==false){
if(keyboard_check_pressed(vk_space)){
bullet = instance_create(x,y,obj_bullet);
bullet.direction = player_direction;
bullet.image_angle = player_direction-90;
bullet.speed = 10;
timer = 20;
start_time=true;
}
}
//}
if(start_time){
timer--;
}
no_shoot = false;

image_angle = player_direction -90;
</string>
