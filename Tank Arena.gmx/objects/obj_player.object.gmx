                break
            }
        else if (instanceid+R).blocked==true
            {
                R--
                break
            }
    }
for (L=0; L&lt;16; L++)//Left raytrace
    {
        if (((instanceid-L).ID-1) mod 16==0)
            {
                break
            }
        else if (instanceid-L).blocked==true
            {
                L--
                break
            }
    }
for (U=0; U&lt;16; U++)//Up raytrace
